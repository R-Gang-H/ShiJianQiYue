/* pages/myAppointment/myAppointment.wxss */
.nav{
  width: 100%;
  overflow: hidden;
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
}
.nav-title{
  width: 100%;
  height: 45px;
  line-height: 36px;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  font-family:PingFang-SC-Medium;
  letter-spacing:2px;
}
.header_tabs {
    display: flex;
    justify-content: center;
    z-index: 1;
}

.header_tabs .item {
  text-align: center;
  position: relative;
}

.header_tabs .item text {
    font-size: 30rpx;
  color: #999999;
  display: inline-block;
  padding: 0 20rpx;
}

.header_tabs .itemed text {
    color: #333333;
    font-size:36rpx;
    font-weight: bold;
}
.cont{
  padding-top:90rpx;
}
.tabs {
    display: flex;
    height: 85rpx;
    line-height: 85rpx;
    border-bottom: #EBECF1 1rpx solid;
    width:100%;
    background:#fff;
    z-index: 1;
}

.tabs .item {
    flex: 1;
    text-align: center;
    position: relative;
}

.tabs .item text {
    font-size: 28rpx;
    color: #999999;
    display: inline-block;
    height: 80rpx;
    padding: 0 20rpx;
}

.tabs .itemed text {
    color: #333333;
    font-weight: bold;
}

.tabs .itemed .h_line {
    width: 66rpx;
    height: 4rpx;
    background: #00B89E;
    position: absolute;
    bottom: -1rpx;
    left: 50%;
    margin-left: -33rpx;
}
/* 
.gsjsContain{
  margin-top:100rpx;
} */
/* .item_in{
    margin-top: 110rpx;
} */
.item_group {
    padding: 20rpx 30rpx;
}

.item_group_list {
    display: flex;
    justify-content: flex-start;
    align-items: top;
}

.item_group_list .icon_pic {
    width: 130rpx;
    height:130rpx;
}

.icon_pic image {
    width: 130rpx;
    height: 130rpx;
    border-radius: 9px;
    overflow: hidden;
}

.group_list {
    margin-left: 25rpx;
    position: relative;
    align-content: space-between;
    display: flex;
    flex-wrap: wrap;
    width: 100%;
    padding: 5rpx 0 10rpx;
    min-height:120rpx;
}

.group_list .list_name {
    display: flex;
    justify-content: flex-start;
    width:100%;
}

.list_name .type_shop {
    width: 85rpx;
    height: 29rpx;
}
.list_name .type_shuji {
    width: 119rpx;
    height: 29rpx;
}

.list_name .name {
    font-size: 28rpx;
    margin-left: 19rpx;
    color: #333333;
    line-height: 38rpx;
}

.group_list .list_time {
    font-size: 24rpx;
    color: #999999;
    display: flex;
    justify-content: flex-start;
    width:100%;
}

.list_time .time {
    margin-right: 19rpx;
}

.group_list .list_content {
    font-size: 26rpx;
    color: #333333;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
}

.red_envelopes {
    width: 97rpx;
    height: 29rpx;
    position: relative;
}

.red_envelopes image {
    width: 97rpx;
    height: 29rpx;
    display: inline-block;
    vertical-align: top;
    position: relative;
}

.red_envelopes .money {
    position: absolute;
    top: 2rpx;
    left: 28rpx;
    font-size: 20rpx;
    color: #DE0006;
    width: 66rpx;
    text-align: center;
}

.list_distance {
    position: absolute;
    bottom: 10rpx;
    right: 0;
    font-size: 24rpx;
    color: #999999;
}

.broadcast {
    font-size: 24rpx;
    color: #666666;
    margin-top: 8rpx;
}

.broadcast_play {
    position: absolute;
    right: 0;
    bottom: 50rpx;
    width: 40rpx;
    height: 40rpx;
}
.goods .group_list{
  padding:0;
}
.goods .list_name{
  display: flex;
  justify-content: space-between;
}
.goods .list_name .name{
  margin-left: 0;
}
.goods .list_name .price{
  font-size: 28rpx;
  color:#333333;
}
.goods .describe{
  font-size: 22rpx;
  color:#999999;
  margin:8rpx 0 0;
  max-height: 130rpx;
  width:100%;
}
.goods .tips{
  width:100%;
}
.goods .tips text{
  font-size: 22rpx;
  color:#333333;
  margin-right:25rpx;
}
.isBorder{
  padding:0 0 20rpx;
  border-bottom:1rpx solid #EEEEEE;
}
.group_bot{
  display: flex;
  justify-content: space-between;
  padding:20rpx 0;
  align-items: center;
}
.group_bot .time{
  font-size:22rpx;
  color:#999999;
}
.group_bot .operate{
  display: flex;
  justify-content: flex-end;
}
.line{
  height:16rpx;
  background: #F5F5F5;
  width:100%;
}
.group_bot .operate .btn{
  height:56rpx;
  line-height: 56rpx;
  font-size:26rpx;
  color:#333333;
  padding:0 30rpx;
  border:1rpx solid #EEEEEE;
  border-radius: 30rpx;
  margin-left: 20rpx;
}