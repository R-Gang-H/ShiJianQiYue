/* pages/sharePrompt/sharePrompt.wxss */
.viewShare{
  font-size: 24rpx;
  color: #999999;
  width: 100%;
  height: 100%;
  position: relative;
}
image{
  width: 188rpx;
  height: 271rpx;
}
.tvShare{
  margin-top: 76rpx;
  margin-left: 70rpx;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  /* 文字位于图片上面 */
  position: absolute;
  left: 0;
  top: 0;
}
.btnShare {
  bottom: 0;
  right: 0;
  left: 0;
  font-size: 30rpx;
  font-family: PingFangSC;
  font-weight: 400;
  height: 88rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  margin-bottom:30rpx;
  color: #fff;
  display: flex;
  position: absolute;
  align-items: center;
  justify-content: center;
  border-radius: 8rpx;
  background-color: #00b89e;
}