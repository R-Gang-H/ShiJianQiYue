/* pages/confirmDue/confirmDue.wxss */
.other1 {
  width: 100%;
  height: 1px;
  background: rgba(238, 238, 238, 1);
}

.infor_list {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 110rpx;
  line-height: 110rpx;
  border-bottom: 1rpx solid #EEEEEE;
  margin-bottom: 37rpx;
}

.infor_list .list_label {
  font-size: 30rpx;
  color: #333333;
  margin-left: 30rpx;
}

.infor_list .list_right {
  display: flex;
  align-items: center;
  margin-right: 15rpx;
}

.weui-input {
  text-align: right;
}

.tvMoney {
  position: absolute;
  right: 0;
  margin-top: 37rpx;
  margin-right: 30rpx;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(0, 184, 158, 1);
}

.tvDesc {
  display: flex;
  align-items: center;
  justify-content: center;

}

.tvWarn {
  margin-top: 12rpx;
  margin-left: 36rpx;
  margin-right: 36rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
}

.btnBuy {
  bottom: 0;
  right: 0;
  left: 0;
  font-size: 30rpx;
  font-family: PingFangSC;
  font-weight: 400;
  height: 88rpx;
  margin-left: 65rpx;
  margin-right: 65rpx;
  margin-bottom: 56rpx;
  color: #fff;
  display: flex;
  position: absolute;
  align-items: center;
  justify-content: center;
  border-radius: 8rpx;
  background-color: #00b89e;
}