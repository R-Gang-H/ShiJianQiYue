/* pages/homeSearch/homeSearch.wxss */

.searchBarView {
  position: fixed;
  display: flex;
  background-color: #fff;
  width: 100%;
  height: 70rpx;
  z-index: 999;
  align-items: center;
  justify-content: center;
}

.searchBar {
  width: 600rpx;
  height: 70rpx;
  background: rgba(245, 245, 245, 1);
  border-radius: 10rpx;
  display: flex;
  padding-left: 30rpx;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: 400;
}

.scrollView {
  top: 70rpx;
  width: 750rpx;
  margin-left: 0rpx;
  position: relative;
}

.scrollView .cell {
}

.cell .cellImageView {
  border-radius: 18rpx;
  position: relative;
  margin-top: 49rpx;
  margin-left: 30rpx;
  width: 130rpx;
  height: 130rpx;
  background-color: red;
}

.cell .titleLabel {
  position: absolute;
  margin-top: 49rpx;
  font-family: PingFang SC;
  font-size: 28rpx;
  color: #333;
  line-height: 46rpx;
  font-weight: 500;
  margin-left: 23rpx;
}

.cell .sjImageView {
  width: 119rpx;
  height: 29rpx;
}

.cell .shImageView {
  width: 85rpx;
  height: 29rpx;
}

.cell .createTimeLabel {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #999;
  margin-left: 23rpx;
  position: absolute;
  margin-top: 149rpx;
}

.cell .expireTimeLabel {
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #999;
}

.cell .distanceLabel {
  position: absolute;
  margin-top: 149rpx;
  right: 32rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #999;
}
