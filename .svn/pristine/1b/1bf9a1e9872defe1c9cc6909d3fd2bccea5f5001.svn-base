/* pages/marketDeatil/marketD.wxss */
/* 轮播图 */
.banner {
  width: 750rpx;
  height: 938rpx;
}

.bannerItem {
  width: 750rpx;
  height: 938rpx;
}

.viewTop {
  width: 100%;
  height: 200rpx;
}


.viewContent{
  padding-bottom: 98rpx;
}
/* 标题 */
.viewHead {
  float: left;
  font-size: 24rpx;
  color: #999999;
  display: flex;
  flex-direction: row;
  width: 100%;
  margin-top: 35rpx;
  margin-bottom: 15rpx;
  position: relative;
  /*父元素位置要设置为相对*/
}

.viewHead .tvTitle {
  /* 文字加粗 */
  font-weight: bold;
  /* 字号 */
  font-size: 28rpx;
  /* 文字颜色 */
  color: #333333;
  /* 左边距 */
  margin-left: 30rpx;
}

.viewHead .imgShare {
  width: 202rpx;
  height: 77rpx;
  position: absolute;
  right: 0;
  /* 靠右调节 */
  margin-right: 26rpx;
}

/* 简介 */
.tvDesc {
  /* 字号 */
  font-size: 22rpx;
  /* 文字颜色 */
  color: #333333;
  /* 上边距 */
  margin-top: 20rpx;
  /* 左边距 */
  margin-left: 30rpx;
}

/* 标题 */
.list_money {
  float: left;
  display: flex;
  flex-direction: row;
  width: 100%;
  margin-top: 10rpx;
  margin-bottom: 15rpx;
  position: relative;
  /*父元素位置要设置为相对*/
}

.list_money .tvMoney {
  /* 文字加粗 */
  font-weight: bold;
  /* 字号 */
  font-size: 50rpx;
  /* 文字颜色 */
  color: #FF3333;
  /* 左边距 */
  margin-left: 30rpx;
}

.list_money .tvSpec {
  /* 字号 */
  font-size: 26rpx;
  /* 文字颜色 */
  color: #333333;
  position: absolute;
  right: 0;
  /* 靠右调节 */
  margin-right: 26rpx;
  margin-top: 15rpx;
}

.other1 {
  height: 14rpx;
  width: 100%;
  background-color: #F6F6F6;
}

/* 地址 */
.viewAddress {
  float: left;
  font-size: 24rpx;
  color: #999999;
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 86rpx;
  /* 文字垂直居中显示 */
  display: flex;
  align-items: center;
  /*  居中显示*/
  /* justify-content: center; */
}

.viewAddress .otherAddress {
  height: 26rpx;
  width: 26rpx;
  margin-left: 30rpx;
}

.viewAddress .tvCityName {
  color: #333333;
  font-size: 26rpx;
  text-align: center;
  margin-left: 22rpx;
}

.other2 {
  height: 14rpx;
  width: 100%;
  margin-top: 86rpx;
  background-color: #F6F6F6;
}

/* 发布者 */
.other3 {
  color: #333333;
  font-size: 33rpx;
  margin-top: 22rpx;
  /* 文字加粗 */
  font-weight: bold;
  margin-left: 22rpx;
}

/* 发布者 */
.viewUser {
  border: 1px solid #E1E1E1;
  display: flex;
  height: 110rpx;
  margin: 30rpx;
  margin-top: 26rpx;
  position: relative;
  /*父元素位置要设置为相对*/
  border-radius: 8rpx;
  /* 文字垂直居中显示 */
  align-items: center;
}

.viewUser .imgAvatar {
  width: 68rpx;
  height: 68rpx;
  border-radius: 25px;
  margin-left: 22rpx;
}

.viewUser .tvName {
  /* 文字加粗 */
  font-weight: bold;
  /* 字号 */
  font-size: 30rpx;
  /* 文字颜色 */
  color: #333333;
  margin-left: 20rpx;
}

.viewUser .btnStar {
  width: 131rpx;
  height: 56rpx;
  border: 2rpx solid rgba(0, 184, 158, 1);
  border-radius: 28rpx;
  position: absolute;
  right: 0;
  /* 靠右调节 */
  margin-right: 20rpx;
  font-size: 26rpx;
  /* 文字颜色 */
  color: #333333;
  /* 居中显示 */
  display: flex;
  align-items: center;
  justify-content: center;
}

.tvComment {
  color: #333333;
  font-size: 33rpx;
  margin-top: 22rpx;
  /* 文字加粗 */
  font-weight: bold;
  margin-left: 22rpx;
  margin-bottom: 15rpx;
}

.viewComment {
  margin-bottom: 85rpx;
  display: inline-block;
}

/* 评论列表 */
.commentList {
  float: left;
  width: 100%;
  margin-top: 34rpx;
  margin-bottom: 15rpx;
  position: relative;
  /*父元素位置要设置为
  相对*/
  display: flex;
}

.commentList .imgUser {
  width: 68rpx;
  height: 68rpx;
  border-radius: 25px;
  margin-left: 22rpx;
}

.viewTv .tvUserName {
  /* 字号 */
  font-size: 26rpx;
  /* 文字颜色 */
  color: #333333;
  margin-left: 20rpx;
  /* 文字加粗 */
  font-weight: bold;
}

.viewTv .tvTime {
  /* 字号 */
  font-size: 20rpx;
  /* 文字颜色 */
  color: #999999;
  margin-top: 12rpx;
  margin-left: 20rpx;
}

.viewTv .tvComment {
  /* 字号 */
  font-size: 24rpx;
  /* 文字颜色 */
  color: #333333;
  margin-top: 25rpx;
  margin-left: 20rpx;
}

/* 底部悬浮 */

.viewFoot {
  box-shadow: 0px 0px 20px 0px rgba(169, 169, 169, 0.2);
  position: fixed;
  width: 100%;
  height: 98rpx;
  background-color: white;
  bottom: 0;
  right: 0;
  left: 0;
  display: flex;
  flex-direction: row;
}

/* 聊天 */
.btnChat {
  border-style: solid;
  border-width: 2rpx;
  border-color: #00b89e;
  border-radius: 8rpx;
  font-size: 30rpx;
  font-family: PingFangSC;
  font-weight: 400;
  width: 320rpx;
  height: 68rpx;
  left: 30rpx;
  margin-top: 15rpx;
