/* pages/set/personalInfor/personalInfor.wxss */
.personal_infor{
  padding:0 30rpx;
}
.infor_list{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 110rpx;
  line-height: 110rpx;
  border-bottom: 1rpx solid #EEEEEE;
}
.infor_list navigator{
  display: flex;
  justify-content: space-between;
  align-items: center;
  width:100%;
}
.infor_list .list_label{
  font-size:30rpx;
  color:#333333;
  flex: 0 0 80rpx;
}
.infor_list .list_right{
  display: flex;
  align-items: center;
} 
.weui-input{
  text-align: right;
}
.list_right .right_text{
  font-size:30rpx;
  color:#999999;
}
.list_right .right_arrow image{
  width: 14rpx;
  height: 25rpx;
}
.disabled .sex{
  color:#999999;
  font-size: 30rpx;
  margin-right: 25rpx;
}
.disabled .birthday{
  color:#999999;
  font-size: 30rpx;
  margin-right: 25rpx;
}
.intro{
  width:100%;
  overflow: hidden;
  white-space: nowrap; 
  text-overflow:ellipsis;
  font-size: 30rpx;

}
.sub{
  margin-top:200rpx;
}
.submit{
  padding:0;
  background:#00B69C;
  text-align: center;
  background: #00B69C;
  height: 78rpx;
  line-height: 74rpx;
  border-radius: 8rpx;
  font-size: 32rpx;
  color: #fff;
  font-weight: normal;
}
.jianjie{
  position: relative;
  width:100%;
}
.jianjie .list_right{
  width:100%;
}
.list_right .intro{
  padding-left:80rpx;
  text-align: right;
  margin-right: 25rpx;
}
.jianjie .list_label{
  position: absolute;
  top:0;
  left:0;
}

